{
  "name": "noodle-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend services for managing noodle JSON files.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "upload:local": "node cli/uploadLocalNoodle.js",
    "validate": "node cli/validateNoodle.js",
    "synthesize": "node cli/synthesize.js",
    "generate": "node cli/genNoodleFromSchema.js",
    "sandbox": "node ../dev/sandbox.js",
    "test": "node test/runTests.js"
  },
  "_notes": {
    "start": "Runs legacy noodle backend on PORT or 4000 using `node server.js`."
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "backblaze-b2": "^2.3.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "tar": "^6.2.0",
    "ws": "^8.14.2"
  }
}
